---
import { metadata } from "@/src/data/meta.data"
---
<head>
  <meta charset="UTF-8" />
  <meta http-equiv="Content-Encoding" content="gzip" />
  <title translate="no">{metadata.tabTitle}</title>
  <link rel="stylesheet" href="/css/style.css" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta property="og:title" content={metadata.tabTitle} />
  <meta property="og:image:width" content="200" />
  <meta property="og:image:height" content="200" />
  <meta property="og:type" content="website" />
  <meta property="og:color" content="#ffffff" />
  <link rel="icon" href={metadata.logo} />
  <link rel="apple-touch-icon" href={metadata.logo} />
  <link rel="apple-touch-icon-precomposed" href={metadata.logo} />
  <link rel="shortcut icon" href={metadata.logo} />
